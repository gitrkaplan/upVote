{
  "manifest_version": 2,

  "name": "upVote",
  "description": "This extension allows the user to add cards to the upVote site.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "upVote"
  },
  "permissions": [
    "activeTab",
      "tabs",
    "storage"
  ]
}
